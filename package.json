{
  "name": "wonky-sprout-os",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start:server": "node server.js",
    "start:prod": "cross-env NODE_ENV=production node server.js",
    "docker:build": "docker build -t wonky-sprout-os:local .",
    "docker:run": "docker run --rm -p 8080:8080 -e PORT=8080 wonky-sprout-os:local",
    "build": "npx vite build",
    "preview": "npx vite preview --port 4173 --strictPort",
    "dev:start": "npm run build && npm run preview",
    "generate:modules": "node scripts/generate/generate_modules_index.mjs",
    "validate:modules": "node -r dotenv/config scripts/generate/validate_module_manifests.mjs",
    "validate:heuristics": "node scripts/generate/validate_heuristic_acronyms.mjs",
    "lint": "eslint . --config eslint.config.cjs --ext .ts,.tsx --report-unused-disable-directives --max-warnings=0",
    "lint:fix": "eslint . --config eslint.config.cjs --ext .ts,.tsx --fix",
    "ci:build": "npm run lint && npm test && npm run build",
    "e2e": "cross-env node ./scripts/run-playwright.js test",
    "test:neurodiversity": "cross-env PLAYWRIGHT_REUSE_EXISTING_SERVER=true npx playwright test tests/e2e/neurodiversity.spec.ts",
    "e2e:stub": "cross-env PLAYWRIGHT_AI_STUB=true node ./scripts/run-playwright.js test",
    "e2e:global": "npm run e2e",
    "e2e:local": "cross-env PLAYWRIGHT_REUSE_EXISTING_SERVER=true npm run e2e",
    "e2e:win": "cross-env NODE_OPTIONS=-r ./tests/e2e/register_shim.cjs npx playwright test",
    "visual:percy": "npx percy exec -- npx playwright test",
    "windows:fix-node": "powershell -ExecutionPolicy Bypass -File ./scripts/windows-node-fix.ps1 -RunInstall",
    "test:coverage": "vitest --coverage",
    "test": "vitest run",
    "test:discord-integration": "vitest run tests/integration --run",
    "test:discord-integration-local": "cross-env DISCORD_INTEGRATION_TEST=true npm run test:discord-integration",
    "test:watch": "vitest",
    "test:ci": "cross-env PLAYWRIGHT_AI_STUB=true npm run e2e",
    "watch:dev": "npx concurrently -k 'npm run dev' 'cross-env PLAYWRIGHT_REUSE_EXISTING_SERVER=true npm run e2e'",
    "dev:watch": "npm run dev",
    "e2e:watch": "npx concurrently -k 'npm run dev' 'cross-env PLAYWRIGHT_REUSE_EXISTING_SERVER=true npm run e2e'",
    "e2e:preview": "bash ./scripts/e2e/start-preview-e2e.sh",
    "e2e:preview:ps": "pwsh -NoProfile -ExecutionPolicy Bypass -File ./scripts/e2e/start-preview-e2e.ps1",
    "e2e:a11y:diagnostics": "bash ./scripts/e2e/run-a11y-diagnostics.sh",
    "e2e:dump-headings": "bash ./scripts/e2e/dump-headings.sh",
    "e2e:show-artifacts": "bash ./scripts/e2e/show-latest-artifact.sh",
    "e2e:show-trace": "bash ./scripts/e2e/show-latest-trace.sh",
    "e2e:open-report": "bash ./scripts/e2e/open-latest-report.sh",
    "e2e:quick": "bash ./scripts/e2e/start-preview-e2e.sh tests/e2e/grandma.spec.ts tests/e2e/airlock.spec.ts",
    "e2e:seed": "node ./scripts/e2e/set-e2e-localstorage.cjs",
    "e2e:seed:default": "node ./scripts/e2e/set-e2e-localstorage.cjs",
    "start:discord-bot": "node ./scripts/start-discord-bot.js",
    "docker:build-discord-bot": "docker build -t wonky-sprout-discord-bot -f Dockerfile.discord-bot .",
    "docker:run-discord-bot": "docker run --rm -e DISCORD_SERVICE_BOT_TOKEN=$DISCORD_SERVICE_BOT_TOKEN -e REDIS_URL=$REDIS_URL wonky-sprout-discord-bot",
    "show-latest-trace": "pwsh -NoProfile -ExecutionPolicy Bypass -File ./scripts/show-latest-trace.ps1",
    "show-latest-trace:node": "node ./scripts/show-latest-trace.js",
    "verify-dashboard": "node ./scripts/verify-dashboard.js",
    "clean": "pwsh -File ./scripts/clean-artifacts.ps1",
    "verify-naming": "node ./scripts/verify-naming.mjs",
    "housekeeping:repo": "node ./scripts/housekeeping/cleanup-repo.cjs",
    "housekeeping:files": "node ./scripts/housekeeping/verify-user-files.cjs",
    "housekeeping": "npm run housekeeping:repo && npm run housekeeping:files",
    "prepare": "husky install",
    "security:scan": "node ./scripts/scan-pii.mjs",
    "format": "prettier --write .",
    "sync-to-ws-os": "node ./scripts/deploy/sync-to-ws-os.mjs"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@sentry/browser": "^7.78.0",
    "@sentry/node": "^7.84.0",
    "@tailwindcss/postcss": "^4.1.17",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dompurify": "^3.0.5",
    "express-rate-limit": "^7.0.1",
    "firebase": "^12.6.0",
    "framer-motion": "^12.23.24",
    "googleapis": "^121.0.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.0",
    "lucide-react": "^0.554.0",
    "marked": "^5.1.1",
    "postcss": "^8.5.6",
    "prom-client": "^15.1.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.6",
    "wonky-sprout-os": "file:"
  },
  "devDependencies": {
    "@babel/parser": "^7.21.8",
    "@eslint/js": "^9.39.1",
    "@percy/cli": "^1.0.0",
    "@percy/playwright": "^1.0.10",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/marked": "^5.0.2",
    "@types/node": "^22.14.0",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/ui": "^4.0.9",
    "autoprefixer": "^10.4.22",
    "axe-core": "^4.8.3",
    "chokidar": "^4.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "firebase-admin": "^11.11.1",
    "firebase-functions": "^3.24.1",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "markdownlint-cli2": "^0.19.1",
    "module-alias": "^2.2.3",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "serve": "^14.1.1",
    "supertest": "^6.3.3",
    "tailwindcss": "^4.1.17",
    "ts-morph": "^27.0.2",
    "tweetnacl": "^1.0.3",
    "typescript": "~5.8.2",
    "vite": "^6.4.1",
    "vitest": "^4.0.9",
    "web-vitals": "^3.3.0",
    "zod": "^3.22.2"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "npm run lint:fix",
      "npm run format",
      "git add"
    ]
  },
  "overrides": {
    "eslint-config-prettier": "^10.1.8",
    "glob": "^11.1.0",
    "rimraf": "^6.0.0"
  }
}
